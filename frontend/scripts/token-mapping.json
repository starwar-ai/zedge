{
  "colorMapping": {
    "primary/500": "primary.500",
    "primary/600": "primary.600",
    "success/500": "success.500",
    "error/500": "error.500",
    "warning/500": "warning.500",
    "info/500": "info.500"
  },
  "spacingMapping": {
    "spacing/4": "spacing.1",
    "spacing/8": "spacing.2",
    "spacing/16": "spacing.4",
    "spacing/24": "spacing.6"
  },
  "fontSizeMapping": {
    "font-size/sm": "text-sm",
    "font-size/base": "text-base",
    "font-size/lg": "text-lg"
  },
  "customTransformations": {
    "colors": {
      "primary": {
        "50": "#f0f9ff",
        "100": "#e0f2fe",
        "500": "#0ea5e9",
        "600": "#0284c7"
      }
    }
  },
  "notes": {
    "description": "Figma Variables 到 Tailwind CSS 的映射配置",
    "usage": "此文件用于自定义 Figma Variables 名称到 Tailwind tokens 的映射规则"
  }
}

